{
    "name": "impensavel/floodgate",
    "type": "library",
    "description": "A PHP library for consuming data from the Twitter Streaming API v1.1 via OAuth",
    "keywords": [
        "twitter",
        "oauth",
        "streaming",
        "api",
        "json"
    ],
    "homepage": "https://github.com/impensavel/floodgate",
    "license": "MIT",
    "authors": [
        {
            "name": "Quetzy Garcia",
            "email": "quetzyg@impensavel.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5.0",
        "guzzlehttp/oauth-subscriber": "0.2.*",
        "guzzlehttp/retry-subscriber": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Impensavel\\Floodgate\\": "src/"
        }
    }
}
